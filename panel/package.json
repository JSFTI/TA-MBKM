  "dependencies": {
    "@tanstack/vue-table": "^8.7.1",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.1",
    "cropperjs": "^1.5.13",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "pinia": "^2.0.27",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@iconify-json/ic": "^1.1.10",
    "@iconify-json/mdi": "^1.1.36",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "@unocss/reset": "^0.47.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue-macros/volar": "^0.5.4",
    "@vue/test-utils": "^2.2.6",
    "eslint": "^8.29.0",
    "jsdom": "^20.0.3",
    "sass": "^1.56.1",
    "taze": "^0.8.4",
    "typescript": "^4.9.3",
    "unocss": "^0.47.5",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "unplugin-vue-macros": "^1.0.3",
    "vite": "^3.2.5",
    "vite-plugin-pages": "^0.27.1",
    "vitest": "^0.25.4",
    "vue-toastification": "2.0.0-rc.5",
    "vue-tsc": "^1.0.11"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "semi": "off",
      "@typescript-eslint/semi": [
        "off"
      ],
      "vue/html-self-closing": [
        "off"
      ],
      "no-empty": "warn",
      "prefer-promise-reject-errors": "off",
      "@typescript-eslint/brace-style": "off",
      "vue/no-extra-parens": "off",
      "no-console": "warn",
      "new-cap": "off",
      "no-alert": "off"
    }
  }
}
